{
  "name": "@keithce/rproj-astrowind",
  "version": "0.1",
  "description": "Resonant Projects - How we bring resonance to your Creative Projects",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "pnpm": {
    "overrides": {
      "path-match": "npm:path-to-regexp@^6.2.1",
      "undici": "^6.21.0",
      "esbuild": "^0.25.0"
    }
  },
  "scripts": {
    "dev": "mkdir -p public && npm run pagefind:build && astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "puppeteer:install": "npx puppeteer browsers install chrome",
    "pagefind:build": "pagefind --site public --output-path public/pagefind || echo 'Pagefind indexing skipped (empty public directory)'",
    "preview": "astro preview",
    "astro": "astro",
    "check": "npm run check:astro && npm run check:eslint && npm run check:prettier",
    "check:astro": "astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w .",
    "lint": "eslint .",
    "format": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "visual-verify": "npm run puppeteer:install && node scripts/visual-verification.js",
    "visual-verify:ci": "npm run puppeteer:install && node scripts/visual-verification.js --headless",
    "test:contrast": "node scripts/contrast-checker.js",
    "test:contrast:palette": "node scripts/contrast-checker.js --palette",
    "validate:css": "node scripts/css-variable-validator.js",
    "test:css-vars": "npm run validate:css",
    "test:all": "npm run test:contrast && npm run validate:css && npm run visual-verify:ci",
    "docs:css": "node scripts/generate-css-docs.js",
    "docs:generate": "npm run docs:css",
    "analyze:content": "npm run puppeteer:install && node scripts/content-analyzer.js",
    "analyze:readability": "node scripts/content-analyzer.js --readability-only",
    "test:accessibility": "npm run puppeteer:install && node scripts/accessibility-test.js",
    "test:accessibility:ci": "npm run puppeteer:install && node scripts/accessibility-test.js --env=development",
    "test:accessibility:staging": "npm run puppeteer:install && node scripts/accessibility-test.js --env=staging",
    "test:accessibility:production": "npm run puppeteer:install && node scripts/accessibility-test.js --env=production",
    "test:accessibility:headed": "npm run puppeteer:install && node scripts/accessibility-test.js --headed",
    "test:accessibility:verbose": "npm run puppeteer:install && node scripts/accessibility-test.js --verbose",
    "axe:scan": "axe http://localhost:4321 --tags wcag2a,wcag2aa,wcag21aa --save axe-results.json",
    "lighthouse:accessibility": "lighthouse http://localhost:4321 --only-categories=accessibility --output-path=accessibility-reports/lighthouse-report.html --view",
    "test:a11y:full": "npm run test:accessibility && npm run axe:scan",
    "test:complete": "npm run test:all && npm run test:accessibility"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/vercel": "^8.2.1",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "1.0.0-beta.8",
    "@calcom/embed-react": "^1.5.3",
    "@fontsource-variable/inter": "^5.2.6",
    "@fontsource-variable/raleway": "^5.2.6",
    "@notionhq/client": "^3.1.3",
    "@pagefind/default-ui": "^1.3.0",
    "@react-email/components": "^0.1.1",
    "@react-email/render": "^1.1.3",
    "@tabler/icons": "^3.34.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.11",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.11.0",
    "astro-cloudinary": "^1.3.2",
    "astro-embed": "^0.9.0",
    "astro-icon": "^1.1.5",
    "astro-mcp": "^0.4.1",
    "autoprefixer": "^10.4.21",
    "botid": "^1.4.0",
    "limax": "4.1.0",
    "lodash.merge": "^4.6.2",
    "notion-client": "^7.4.2",
    "notion-utils": "^7.4.2",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.6",
    "resend": "^4.6.0",
    "sirv": "^3.0.1",
    "sirv-cli": "^3.0.1",
    "swiper": "^11.2.10",
    "tailwind-variants": "^1.0.0",
    "tw-animate-css": "^1.3.4",
    "unpic": "^4.1.2",
    "vercel": "^44.4.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdown-remark": "6.3.2",
    "@astrojs/mdx": "4.3.0",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.3.0",
    "@axe-core/cli": "^4.10.2",
    "@eslint/js": "^9.30.0",
    "@iconify-json/flat-color-icons": "^1.2.1",
    "@iconify-json/material-symbols": "^1.2.28",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/tabler": "^1.2.19",
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "astro-compress": "2.3.8",
    "astro-eslint-parser": "^1.2.2",
    "axe-core": "^4.10.3",
    "eslint": "^9.30.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.2.0",
    "js-yaml": "^4.1.0",
    "lighthouse": "^12.7.0",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "puppeteer": "^24.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reading-time": "^1.5.0",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "unist-util-visit": "^5.0.0"
  }
}
